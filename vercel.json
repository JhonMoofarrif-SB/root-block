{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run build && pnpm --filter @seguros-bolivar-ui/docs run build-storybook && mkdir -p packages/docs/storybook-static/examples && cp -r examples/* packages/docs/storybook-static/examples/",
  "outputDirectory": "packages/docs/storybook-static",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/examples/:path*",
      "destination": "/examples/:path*"
    }
  ],
  "headers": [
    {
      "source": "/examples/dist/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/examples/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}
